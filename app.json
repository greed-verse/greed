{
  "expo": {
    "name": "GREED",
    "slug": "GREED",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/GreedLogo.png",
    "scheme": "greed",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/GreedRedLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0F13"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Greed",
      "statusBar": {
        "hidden": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0F0F13"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/GreedRedLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0F0F13"
        }
      ],
      "@azesmway/react-native-unity"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
